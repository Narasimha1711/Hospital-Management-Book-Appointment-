.nav_link {
  position: relative;
  /* background-color: rgb(177, 39, 39); */
}

.nav_link::after {

  content: "";
  height: 4.5px;
  width: 0%;
  border-radius: 6px;
  background-color: rgb(0, 183, 255);
  display: inline-block;
  position: absolute;
  left: 20%;
  bottom: -8px;
  opacity: 0;
  transition: ease .5s;
  transition: width .9s ease;



}

.nav_link:hover::after {
  opacity: 1;
  /* font-size: 22px; */

  width: 60%;
